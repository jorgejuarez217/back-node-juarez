 <table class="table table-bordered border-dark" style="margin:3rem 0;">
        <thead class="thead-dark" style="background-color:aliceblue ;">
            <tr>
                <th scope="col">Nombre</th>
                <th scope="col">Precio</th>
                <th scope="col">Cantidad</th>
            </tr>
        </thead>
        <tbody>
            {{#each productos}}

                <tr scope="row">
                    <td>
                            {{title}} 
                    </td>
                    <td>
                            {{price}}
                    </td>
                    <td>
                            {{cantidad}}   
                    </td>
                    
                </tr>  
            
            {{/each}}
        </tbody>
    </table>